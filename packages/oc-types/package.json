{
  "name": "@opencollection/types",
  "version": "0.0.1",
  "description": "TypeScript types for OpenCollection schema",
  "author": "Bruno Software Inc.",
  "main": "src/opencollection.ts",
  "types": "src/opencollection.ts",
  "files": [
    "src",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./src/opencollection.ts",
      "default": "./src/opencollection.ts"
    },
    "./OpenCollection": {
      "types": "./src/opencollection.ts",
      "default": "./src/opencollection.ts"
    },
    "./opencollection": {
      "types": "./src/opencollection.ts",
      "default": "./src/opencollection.ts"
    },
    "./common/*": {
      "types": "./src/common/*.ts",
      "default": "./src/common/*.ts"
    },
    "./config/*": {
      "types": "./src/config/*.ts",
      "default": "./src/config/*.ts"
    },
    "./collection/*": {
      "types": "./src/collection/*.ts",
      "default": "./src/collection/*.ts"
    },
    "./requests/*": {
      "types": "./src/requests/*.ts",
      "default": "./src/requests/*.ts"
    }
  },
  "license": "MIT",
  "keywords": [
    "opencollection",
    "types",
    "typescript",
    "api",
    "http"
  ]
}

